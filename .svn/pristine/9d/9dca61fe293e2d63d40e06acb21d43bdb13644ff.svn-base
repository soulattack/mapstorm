    function addbooks() {
        if (isVip == false) {
            tip();
            return;
        }
        api.confirm({
            title: '提示',
            msg: '确定添加' + books.length + '位联系人到通讯录 ',
            buttons: ['确定', '取消']
        }, function (ret, err) {
            var list = books;
            if (ret.buttonIndex == 1) {
                if (ret) {
                    var inum = 0;
                    for (var i = 0; i < list.length; i++) {
                        if (list[i]) {
                            var contacts = api.require('DVContacts');
                            contacts.add({
                                firstName:"城市客源"+(i+1)+"-"+list[i],
                                phones: [{
                                    '工作': list[i]
                                }]
                            }, function (ret, err) {
                                if (ret) {
                                    if (ret.status == true) {
                                        console.log("cegong")
                                        inum++;
                                    }
                                } else {
                                    showTip(JSON.stringify(err), 2000, "middle");
                                }
                            });
                        }
                        if (i == list.length - 1) {
                            showTip("成功导入" + (i + 1) + "个联系人", 2000, "middle");
                        }
                    }
                }
            }
        })
    }

